<ul class="dropdown">
    {% for f in favoris %}
        {% if not loop.last %}
            <li><a href="{{ path('app_bien_recherche', {'id': f.id}) }}">{{ f.type }}</a></li>
        {% else %}
            <li class="last-item"><a href="{{ path('app_bien_recherche', {'id': f.id}) }}">{{ f.type }}</a></li>
        {% endif %}
    {% endfor %}
</ul>