                          <a class="" href="{{ path('bien.show', {'id': bien.id}) }}" title="{{ bien.price|number_format(0, ',', ' ') ~ ' FCFA '}}{{bien.paiementLouer ? '/'~ bien.paiementLouer :''}}">
